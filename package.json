{
  "name": "@unional/assert-order",
  "version": "1.1.0",
  "main": "dist/commonjs/index.js",
  "typings": "dist/commonjs/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run build-commonjs && npm run build-global",
    "build-commonjs": "tsc -p tsconfig.commonjs.json",
    "build-global": "webpack --config webpack.config.dev.js",
    "clean": "rimraf dist",
    "coverage": "npm test && nyc check-coverage --lines 85 --functions 85 --branches 85",
    "dependency-check": "dependency-check . --unused --no-dev && dependency-check . --missing --no-dev",
    "lint": "tslint \"src/**/*.ts\"",
    "postpublish": "git push",
    "prepare": "typings install && npm run build",
    "preversion": "git pull && npm run verify",
    "test": "npm run clean && tsc && nyc ava",
    "verify": "npm run lint && npm run coverage && npm run build && npm run dependency-check",
    "watch": "npm run clean && node bin/watch.js"
  },
  "license": "MIT",
  "ava": {
    "files": [
      "dist/spec/**/*.spec.js"
    ],
    "source": [
      "bin/*.js",
      "dist/spec/**/*.js",
      "*.json"
    ],
    "require": [
      "./bin/setup-browser-env.js",
      "./bin/setup-test-env.js"
    ]
  },
  "nyc": {
    "exclude": [
      "bin",
      "**/*.spec.*",
      "**/fixtures/**/*"
    ]
  },
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "peerDependencies": {
    "@unional/logging": ">=0.1.1"
  },
  "devDependencies": {
    "@unional/logging": "^0.1.1",
    "aurelia-polyfills": "^1.1.1",
    "ava": "^0.17.0",
    "browser-env": "^2.0.19",
    "core-js": "^2.4.1",
    "dependency-check": "^2.6.0",
    "eslint": "^3.12.2",
    "eslint-config-unional": "^0.2.3",
    "nyc": "^10.0.0",
    "pascal-case": "^2.0.0",
    "rimraf": "^2.5.4",
    "ts-loader": "^1.3.3",
    "tslint": "^4.1.1",
    "tslint-config-unional": "^0.6.0",
    "typescript": "^2.1.4",
    "typings": "^2.1.0",
    "webpack": "^1.14.0",
    "webpack-fail-plugin": "^1.0.5"
  }
}
